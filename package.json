{
  "name": "@npkgdev/nestjs-request-trace",
  "version": "1.0.0",
  "description": "Middleware for managing X-Request-Id and X-Correlation-Id headers in NestJS",
  "author": "Douglas de Paula Natalli",
  "license": "MIT",
  "url": "https://github.com/npkgdev/nestjs-request-trace#readme",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prepare": "husky install",
    "prerelease": "npm run build",
    "release": "npx semantic-release"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-angular": "17.7.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/testing": "8.4.7",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.1.0",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "18.17.6",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "lint-staged": "14.0.0",
    "prettier": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "semantic-release": "19.0.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "express": "^4.17.2",
    "reflect-metadata": "^0.1.13"
  },
  "publishConfig": {
    "@npkgdev:registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npkgdev/nestjs-request-trace"
  }
}
